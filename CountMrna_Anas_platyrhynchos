# Telt aantal a,t,g,c
aantal_a = mRNA.count("A")
aantal_t = mRNA.count("T")
aantal_g = mRNA.count("G")
aantal_c = mRNA.count("C")

print("aantal a:")
print(aantal_a)
print("aantal c:")
print(aantal_c)
print("aantal g:")
print(aantal_g)
print("aantal t:")
print(aantal_t)

aantal_totaal = aantal_t + aantal_g + aantal_c + aantal_a

print("aantal totaal:")
print(aantal_totaal)

# gc% = (g+c) / totaal * 100 | Opdracht 2
gc_pombe = (aantal_g + aantal_c)/aantal_totaal * 100
print(str(round(gc_pombe, 2)) + "%")
